<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.joseph.safewalk2">

    <application
        android:allowBackup="true"
        android:icon="@drawable/banner_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <uses-permission android:name="android.permission.SEND_SMS" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    <activity android:name=".Details"></activity>
    <activity android:name=".LocationDeliverous"></activity>
    <activity android:name=".UserAccount"></activity>
    <activity android:name=".Register"></activity>
    <activity android:name=".Buildous"></activity>


    </application>

</manifest>